{% extends '::base.html.twig' %}
{% block title %}TECNICAMENTE 2016!!!{% endblock %}
{% block body %}

<nav class="navbar navbar-default">
  <img src="{{ asset('public/img/imgtm1.jpg') }}" alt="..." class="img-responsive">s
  <div class="container-fluid">
      <ul class="nav navbar-nav">
        <li class="active"><a href="{{path('Inicio')}}">Volver <span class="sr-only">(current)</span></a></li>
      </ul>
  </div><!-- /.container-fluid -->
</nav>

<section class="Container">
	<div class="row">
		<div class="col-md-12">
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3>TécnicaMente 2016</h3>
				</div>
				<div class="panel-body">
				    <h3>Lista de Presentaciones "Experiencia Pedagógica"</h3>
                    <table class="table table-hover">
                            <tr class="danger">
                                <th>Establecimiento</th>
                                <th>Año de Desarrollo</th>
                                <th>Categoría</th>
                                <th>Especificación</th>
                            </tr>
                            {% for pres in presentacion %}
                            <tr class="danger">
                                <th>{{pres.escuela.nombesc}}</th>
                                <th>{{pres.adpresentacion}}</th>
                                <th>{{pres.catpresentacion}}</th>
                                <th>{{pres.esppresentacion}}</th>
                            </tr>
                            {% endfor %}
                    </table>				
				</div>
			</div>
		</div>
	</div>
</section>
{% endblock %}